{
    "name": "LDS Devtools Cache Visualization",
    "version": "1.0",
    "description": "DevTools extension for data visualization of LDS cache.",
	  "manifest_version": 2,
    "permissions": [
		  "activeTab",
      "tabs",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "devtools_page": "devtools.html"

}
